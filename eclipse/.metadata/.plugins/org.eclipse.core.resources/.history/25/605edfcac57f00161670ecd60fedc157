package com.derf.btawc.items;

import java.util.List;

import net.minecraft.entity.Entity;

public abstract class ItemMagnet extends ItemBasic {

	private List<Class<? extends Entity>> entites;
	private int range;
	private int strength;
	
	
	public ItemMagnet(String name, List<Class<? extends Entity>> entities, int range, int strength) {
		super(name);
	}

}
